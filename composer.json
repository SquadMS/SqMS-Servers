{
    "name": "squadms/servers",
    "description": "The SquadMS Server management module.",
    "keywords": ["laravel", "squad", "squadms", "servers"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://pkg.skyraptor.eu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/cache": "^8.0",
        "nesbot/carbon": "^2.0",
        "yajra/laravel-datatables-oracle": "~9.0",
        "dsg/squad-rcon-php": "^1.0.0",
        "austinb/gameq": "^3.0.15",
        "squadms/foundation": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\Servers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquadMS\\Servers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\Servers\\ServersServiceProvider"
            ]
        }
    }
}
